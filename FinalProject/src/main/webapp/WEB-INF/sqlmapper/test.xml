<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 
 <mapper namespace="mytestmembermapper">
 	
	<select id="selectbyusername" resultType="securityTest.MemberVO">
		select * from dream.member where m_email=#{username}
	</select>
	
	<select id="selectbyadminname" resultType="securityTest.AdminVO">
		select* from dream.admin where admin_email=#{username}
	</select>
 	
 </mapper>
 